include ../mixins.pug

doctype html
html(lang="en")
head
  block metadata
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
  block title
    title Document
  block links
    link(rel="stylesheet", href="css/styles.min.css")
body
  block header
    include ../includes/header.pug
  block main
    main.main
  block footer
    include ../includes/footer.pug
  block scripts
    script(src="js/script.min.js")
